{
    "Aftonbladet": {
      "Uri": "http://www.aftonbladet.se/rss.xml",
      "Filters": [
        {
          "MatchOn": "Title",
          "Type": "Contains",
          "Value": "kardashian",
          "Negative": true
        }
      ],
      "Items": [
        {
          "Name": "Regular news",
          "Filters": [
            {
              "MatchOn": "Category",
              "Type": "Matches",
              "Value": "nyheter"
            }
          ]
        },
        {
          "Name": "Sports without soccer",
          "Filters": [
            {
              "MatchOn": "Category",
              "Type": "Contains",
              "Value": "sportbladet"
            },
            {
              "MatchOn": "Category",
              "Type": "Contains",
              "Value": "fotboll",
              "Negative": true
            }
          ]
        }
      ]
    }
  }